        </dependency>
        <dependency>
            <groupId>io.shulie.tro</groupId>
            <artifactId>tro-cloud-k8s</artifactId>
        </dependency>
        <dependency>
            <groupId>io.shulie.tro</groupId>
            <artifactId>tro-cloud-biz-service</artifactId>
        </dependency>
    </dependencies>

</project>
